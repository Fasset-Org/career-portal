                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="card recent-sales overflow-auto">
                            <div class="filter">
                                <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
                                <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                                    <li class="dropdown-header text-start">
                                        <h6>Action</h6>
                                    </li>
                                    <li><a class="dropdown-item" href="#">View</a></li>
                                </ul>
                            </div>
                            <div class="card-body">
                                <h5 class="card-title">Learner Opportunities</h5>
                                <table class="table table-borderless datatable">
                                    <thead>
                                        <tr>
                                            <th scope="col">Posted</th>
                                            <th scope="col">Province</th>
                                            <th scope="col">Description</th>
                                            <th scope="col">Type</th>
                                            <th scope="col">Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var Opportunities in Model.Opportunities)
                                        {
                                            <tr>
                                                <th scope="row"><a href="#">@Opportunities.PostedDate.ToString("yyyy-MM-dd")</a></th>
                                                <td>@Opportunities.Province</td>
                                                <td><a href="#" class="text-primary">@Opportunities.Description</a></td>
                                                <td>@Opportunities.Type</td>
                                                <td>
                                                    @if (Opportunities.Status == "Open")
                                                    {
                                                        <span class="badge bg-success">@Opportunities.Status</span>
                                                    }
                                                    else if (Opportunities.Status == "Closed")
                                                    {
                                                        <span class="badge bg-danger">@Opportunities.Status</span>
                                                    }
                                                    else if (Opportunities.Status == "Closing Soon")
                                                    {
                                                        <span class="badge bg-warning">@Opportunities.Status</span>
                                                    }
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="card top-selling overflow-auto">
                            <div class="filter">
                                <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
                                <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                                    <li class="dropdown-header text-start">
                                        <h6>Action</h6>
                                    </li>
                                    <li><a class="dropdown-item" href="#">View</a></li>
                                </ul>
                            </div>
                            <div class="card-body pb-0">
                                <h5 class="card-title"> Recent Applications<span>| Latest</span></h5>
                                <table class="table table-borderless">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">Description</th>
                                            <th scope="col">Applied</th>
                                            <th scope="col">Status</th>
                                            <th scope="col">Type</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{

                                            int counter = 1;
                                            foreach (var application in Model.RecentApplication)
                                            {
                                                <tr>
                                                    <th>@counter</th>
                                                    <td>@application.ApplicationDesc</td>
                                                    <td>@application.ApplicationAppliedDate.ToString("yyyy-MM-dd")</td>
                                                    <td>@application.ApplicationStatus</td>
                                                    <td>@application.ApplicationType</td>
                                                </tr>
                                                counter++;
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
