                                    </div>
                                    <div class="ps-3">
                                        <h6>@ViewBag.DocumentsPerce<span> %</span> </h6>
                                        <span class="text-danger small pt-1 fw-bold"></span> <span class="text-muted small pt-2 ps-1">COMPLETED</span>
                                    </div>
